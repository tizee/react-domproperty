{
  "name": "react-domproperty",
  "version": "1.0.6",
  "description": "Convert HTML DOM attributes to React DOM attributes",
  "author": "tizee <pobomp@gmail.com>(https://tizee.github.io)",
  "license": "MIT",
  "main": "dist/convert.js",
  "types": "dist/types/convert.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ts-check": "tsc --noEmit",
    "ts-check:watch": "tsc --noEmit --watch",
    "build": "tsc && npx rollup -c && node ./scripts/build.js",
    "doc": "node ./scripts/build.js",
    "dev": "npx rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "npm run test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/jest": "^26.0.4",
    "@types/preval.macro": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-plugin-preval": "^5.0.0",
    "codecov": "^3.7.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "preval.macro": "^5.0.0",
    "rollup": "^2.16.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-uglify": "^6.0.4",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "keywords": [
    "react",
    "html",
    "dom",
    "attribute",
    "convert",
    "react-domproperty"
  ],
  "homepage": "https://github.com/tizee/react-domproperty#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tizee/react-domproperty"
  },
  "bugs": {
    "url": "https://github.com/tizee/react-domproperty"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
